Package: UrnModel
Type: Package
Title: HBSm Simulation
Version: 0.9.0
Authors@R: 
  c(person(given = "Nicolas L.",
           family = "Harder",
           role = c("aut", "cph"),
           email = "NHARDER@email.sc.edu"),
           person(given = "Gage A.",
           family = "Pierce",
